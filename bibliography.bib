@Article { kefeer.s:2007:closing,
	title = {In Closing},
	subtitle={amen and doxology in {Anglo-Saxon England}},
	author = {Keefer, Sarah Larratt},
	journaltitle = {Anglia~— Zeitschrift für englische Philologie},
	volume = {121},
	number = {2},
	pages = {210–237},
	year = {2007},
	file = {[[/home/me/library/Kefeer, S. L.; 2007; In closing.pdf]]},
	abstract={In this study we explore the Anglo-Saxon understanding of and translations for ‘amen’ and ‘doxology’ as they are found at the end of homiletic, scriptural and above all, liturgical texts of the period. In some traditions, ‘amen’ is mereley repeated without translation as a closure marker in its own language, while in others it is rendered in several different ways; these are notably a ‘correct’ version using a subjunctive which indicates knowledge of ‘amen’s literal significance, and an ‘incorrect’ form that confuses the vernacular adverbial discourse marker ‘soðlice’ with the closure marker proper. In poetry, both ‘amen’ and any version of a closing doxological formula are frequently amplified, and comparison of these provides a new perception of the way in which Anglo-Saxons apprehended and used traditional liturgical closure markers.}
}


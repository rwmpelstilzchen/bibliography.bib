@Article{ olsen.k:2004:beggar,
	author = {Olsen, Karin E.},
	title = {Beggars’ Saint but no Beggar: Martin of Tours in Ælfric’s Lives of Saints},
	journaltitle = {Neophilologus},
	year = 2004,
	volume = {88},
	number = {3},
	pages = {461–475},
	abstract = {Although Ælfric’s longer Life of St Martin is clearly indebted to Sulpicius Severus’s writings, Martin’s humility, one of the chief virtues for which the saint is remembered, is downplayed in Lives. Ælfric’s Martin also leads a life of self-deprivation, yet without humiliating himself in the same way as his Latin counterpart does. Ælfric silently passes over Martin’s shabby appearance and servile behaviour towards men of inferior rank, thus omitting those details that would compromise Martin’s status as Bishop of Tours. For Ælfric, the saint’s authority as a high ecclesiastical official needed to be left untainted in order to make his role conform with the prestigious position bishops enjoyed in the political and spiritual life of late Anglo-Saxon England.},
	file = {[[/home/me/library/Olsen. K.E.; 2004; Beggars’ Saint but no Beggar — Martin of Tours in Ælfric’s Lives of Saints.pdf]]},
